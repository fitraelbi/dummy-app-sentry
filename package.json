{
  "name": "express-crud-app",
  "version": "1.0.0",
  "description": "CRUD API with Express.js",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "docker:build": "docker build -t express-crud:latest .",
    "docker:run": "docker run -p 3000:3000 express-crud:latest",
    "docker:compose": "docker-compose up -d",
    "k8s:deploy": "kubectl apply -f kubernetes/deployment.yaml",
    "k8s:delete": "kubectl delete -f kubernetes/deployment.yaml"
  },
  "keywords": [
    "express",
    "crud",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sentry/node": "^10.17.0",
    "@sentry/profiling-node": "^10.17.0",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  }
}
